def calculate_final_score(skill_score, experience_years):

    if experience_years == 0:
        # Fresher scoring
        final_score = skill_score * 0.7
        candidate_type = "Fresher"
    else:
        # Experienced scoring
        final_score = skill_score * 0.5 + (experience_years * 5)
        candidate_type = "Experienced"

    return round(final_score, 2), candidate_type
